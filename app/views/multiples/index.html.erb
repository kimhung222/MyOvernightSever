
<center>
<h1>Listing Yes-No questions</h1>

<td><%= link_to 'Create a Multiple Choice questions', new_multiple_path %>
 
<table>
  <tr>
    <th>Index</th>
    <th>Field</th>
    <th>Score</th>
  </tr>
 
  <% @multiples.each do |multiple| %>
    <tr>
      <th><%= multiple.id %></th>
      <th><%= multiple.field %></th>
      <th><%= multiple.score %></th>
      <th><%= link_to 'Show', multiple_path(multiple) %></th>
      <th><%= link_to 'Edit', edit_multiple_path(multiple) %></th>
      <th><%= link_to 'Destroy', multiple_path(multiple),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></th>
    </tr>
  <% end %>
</table>
</center>